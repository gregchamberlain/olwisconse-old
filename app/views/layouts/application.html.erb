<!DOCTYPE html>
<html>
  <head>
    <title>Ol' Wisconse</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <script>
      var user = <%= current_user ? (render '/api/sessions/user.json.jbuilder', user: current_user).html_safe : {} %>
      window.currentUser = user.id ? user : null
    </script>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= yield %>
  </body>
</html>
